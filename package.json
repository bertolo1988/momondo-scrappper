{
  "name": "momondo-scrappper",
  "version": "0.0.1",
  "description": "A tool to query data from momondo.com using phantomjs-node",
  "main": "dist/momondo-scrappper.js",
  "engines": {
    "node": ">= 6.5.0"
  },
  "scripts": {
    "test": "mocha -r ts-node/register test/**.ts",
    "postinstall": "typings install",
    "build": "tsc -p .",
    "watch": "tsc -w",
    "prepublish": "npm run build",
    "verbose-start": "cross-env DEBUG=* node ./dist/momondo-scrappper.js",
    "start": "cross-env DEBUG=momondo-scrappper node ./dist/momondo-scrappper.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bertolo1988/momondo-scrappper.git"
  },
  "keywords": [
    "momondo"
  ],
  "author": "bertolo1988 <tiagobertolo@gmail.com> (https://github.com/bertolo1988)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bertolo1988/momondo-scrappper/issues"
  },
  "homepage": "https://github.com/bertolo1988/momondo-scrappper#readme",
  "devDependencies": {
    "chai": "3.5.0",
    "mocha": "3.1.2",
    "ts-node": "1.6.1",
    "typescript": "2.0.6"
  },
  "dependencies": {
    "cross-env": "3.1.3",
    "debug": "2.2.0",
    "phantom": "3.0.2"
  }
}